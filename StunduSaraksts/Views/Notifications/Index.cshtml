@model IEnumerable<StunduSaraksts.ModelsDB.Notification>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SentTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ReceivedTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ContentNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RecipientNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SenderNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SentTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ReceivedTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ContentNavigation.Text)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RecipientNavigation.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SenderNavigation.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
